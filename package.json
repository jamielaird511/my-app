{
  "name": "my-app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20 <=22"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "verify": "npm run typecheck && npm run lint && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --runInBand",
    "prepare": "husky install"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.54.0",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/estree": "^1.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.9.1",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "8.51.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.5.2",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "next lint --max-warnings=0"
  }
}
